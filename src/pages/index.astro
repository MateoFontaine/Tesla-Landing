---
import LandingHeader from "../components/LandingHeader.astro";
import Layout from "../layouts/Layout.astro";
import HeroSection from "../components/HeroSection.astro";
import ModelY from "../components/ModelYSection.astro"
import ModelS from "../components/ModelSSection.astro";
import Model3 from "../components/Model3Section.astro";
import ModelX from "../components/ModelXSection.astro"
import Accesories from "../components/AccesoriesSection.astro"
import Wall from "../components/WallSection.astro"
import CybertruckSection from "../components/CybertruckSection.astro";
---

<Layout title="Tesla - El coche de tus suenos">
  <LandingHeader />
  <main class="snap-y snap-mandatory relative w-full h-screen overflow-auto scroll-m-0">
	<div class="snap-center">
		<HeroSection />
	</div>
	<div class="snap-center">
		<ModelS/>
	</div>
	<div class="snap-center">
		<Model3/>
	</div>
	<div class="snap-center">
		<ModelX/>
	</div>
	<div class="snap-center">
		<ModelY />
	</div>
	<div class="snap-center">
		<CybertruckSection/>
	</div>
	<div class="snap-center">
		<Wall/>
	</div>
	<div class="snap-center">
		<Accesories/>
	</div>
	


    
  </main>
</Layout>
